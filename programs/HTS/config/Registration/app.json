{
    "Age": {
        "fieldType": "period",
        "validationRule": "^[1-9][0-9]?[D|W|M|Y]$",
        "validationMessage": "Expecting valid ages greater than 0 by units like 11D for 11 days!",
        "hiddenButtons": ["clear", "/", ".", "-", "abc", "qwe", "Unknown"]
    },
    "First Name": {
        "ajaxURL": "/dde/list_first_names?name=",
        "fieldType": "text"
    },
    "Last Name": {
        "ajaxURL": "/dde/list_last_names?name=",
        "fieldType": "text"
    },
    "Gender": {
        "options": [
            "Female",
            "Male"
        ]
    },
    "Ask Middle Name?": {
        "answer": "No",
        "visible": false
    },
    "Ask Date of Birth?": {
        "answer": "Yes",
        "visible": false
    },
    "Ask Nationality?": {
        "answer": "No",
        "visible": false
    },
    "Ask Home District?": {
        "answer": "Yes",
        "visible": false
    },
    "Ask Current Residence?": {
        "answer": "Yes",
        "visible": false
    },
    "Ask Occupation?": {
        "answer": "No",
        "visible": false
    },
    "Ask Cellphone Number?": {
        "answer": "Yes",
        "visible": false
    },
    "Ask Home Phone Number?": {
        "answer": "No",
        "visible": false
    },
    "Ask Office Phone Number?": {
        "answer": "No",
        "visible": false
    },
    "Region of Origin": {
        "ajaxURL": "/dde/search_by_region?name="
    },
    "Current Region": {
        "ajaxURL": "/dde/search_by_region?name=",
        "autoNext": true
    },
    "Year of Birth": {
        "fieldType": "number",
        "validationRule": "^\\d{4}$|Unknown",
        "min": "thisYear - 120",
        "max": "thisYear",
        "validationMessage": "Birth year \n must be (current year - 110) to (current year)",
        "hiddenButtons": ["clear", "/", ".", "-", "abc", "qwe"]
    },
    "Year of Birth Unknown?": {
        "visible": false,
        "func": ["Year of Birth", "match", "Unknown"]
    },
    "Month of Birth": {
        "options": ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December", "Unknown"],
        "className": "longSelectList",
        "title": "Missing Data",
        "message": "Month of birth \n must be selected"
    },
    "Month of Birth Unknown?": {
        "visible": false,
        "func": ["Month of Birth", "match", "Unknown"]
    },
    "Date of Birth": {
        "fieldType": "days",
        "yearField": "Year of Birth",
        "monthField": "Month of Birth",
        "title": "Missing Data",
        "message": "Date of birth \n must be entered",
        "validationRule": "^\\d+$|^Unknown$",
        "validationMessage": "Date of birth \n must be entered"
    },
    "Date of Birth Unknown?": {
        "visible": false,
        "func": ["Date of Birth", "match", "Unknown"]
    },
    "Ask T/A of Origin?": {
        "answer": "No",
        "visible": false
    },
    "Ask Closest Landmark?": {
        "answer": "No",
        "visible": false
    },
    "Cell Phone Number": {
        "fieldType": "number",
        "validationRule": "^0[2|8|9]\\d{8}$|Unknown|Not Available|^N\\/A$",
        "validationMessage": "Cellphone number \n must start with 02,08 or 09 \n followed by 8 digits",
        "hiddenButtons": ["clear", "/", ".", "-", "abc", "qwe"]
    },
    "District of Origin": {
        "ajaxURL": "/dde/search_by_district?region=REGION&district=",
        "ajaxURLDummies": {
            "Region of Origin": "REGION",
            "autoNext": true
        },
        "fieldType": "text"
    },
    "Current District": {
        "ajaxURL": "/dde/search_by_district?region=REGION&district=",
        "ajaxURLDummies": {
            "Current Region": "REGION",
            "autoNext": true
        },
        "fieldType": "text"
    },
    "Current T/A": {
        "ajaxURL": "/dde/search_by_t_a?district=DISTRICT&ta=",
        "ajaxURLDummies": {
            "Current District": "DISTRICT",
            "autoNext": true
        },
        "fieldType": "text"
    },
    "Current Village": {
        "ajaxURL": "/dde/search_by_village?ta=TA&village=",
        "ajaxURLDummies": {
            "Current T/A": "TA",
            "autoNext": true
        },
        "fieldType": "text"
    },
    "Patient Match Found?": {
        "customComponent": "DDESearch",
        "properties": {
            "label": "Select or Create New Client"
        }
    },
    "Scan barcode": {
        "fieldType": "barcode",
        "targetUrl": "/programs/scan/"
    },
    "Child?": {
        "visible": false,
        "func": ["age", "inRange", "0", "14", "Other"]
    },
    "Client consents to be contacted": {
        "title": "Missing Data",
        "message": "Client consent \n Select Yes or No"
    },
    "Search by name?": {
        "title": "Missing Data",
        "message": "Search by name \n Select Yes or No"
    },
    "action": "/programs/save_demographics"
}